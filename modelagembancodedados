select * from produto 
where quantidade = 0 and VALOR < 100
and UNIDADE  = 'UN' and DESCRICAO like '%63%'
order by QUANTIDADE  

select * from produto 
where QUANTIDADE >= 5 and QUANTIDADE <= 50;

select *,(quantidade * valor) as total from produto 
where DESCRICAO not like '%bola%'

select * from produto
where  QUANTIDADE <> 5 and QUANTIDADE > 1 and QUANTIDADE  <10
